{
  "name": "gtav-server",
  "version": "0.0.1",
  "scripts": {
    "format": "prettier --write {src,scripts}/**/*.ts",
    "build:production": "rollup -c ./scripts/rollup.config.js --environment NODE_ENV:production",
    "build": "rollup -c ./scripts/rollup.config.js",
    "watch": "rollup -w -c ./scripts/rollup.config.js",
    "dev": "ragemp-server.exe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exodie/gtav-server.git"
  },
  "license": "MIT",
  "homepage": "https://github.com/exodie/gtav-server#readme",
  "devDependencies": {
    "@ragempcommunity/types-client": "^2.0.6",
    "@ragempcommunity/types-server": "^2.0.6",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@swc/core": "^1.2.197",
    "@swc/helpers": "^0.3.17",
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.41",
    "@types/nodemailer": "^6.4.4",
    "bcryptjs": "^2.4.3",
    "builtin-modules": "^3.3.0",
    "fs-jetpack": "^4.3.1",
    "nodemailer": "^6.7.5",
    "prettier": "^2.6.2",
    "rollup": "^2.75.6",
    "rollup-plugin-swc3": "^0.3.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-tsconfig-paths": "^1.1.8",
    "rollup-plugin-typescript2": "^0.32.1",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "colorette": "^2.0.17",
    "dotenv": "^16.0.1",
    "keycode": "^2.2.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.3.6",
    "rage-rpc": "^0.4.0"
  },
  "prettier": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "endOfLine": "lf",
    "printWidth": 150,
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": true,
    "overrides": [
      {
        "files": "*.yml",
        "options": {
          "tabWidth": 2,
          "useTabs": false
        }
      }
    ]
  }
}
